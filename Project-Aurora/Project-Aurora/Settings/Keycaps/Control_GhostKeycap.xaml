<UserControl x:Class="Aurora.Settings.Keycaps.Control_GhostKeycap"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="30" HorizontalAlignment="Left" VerticalAlignment="Top">
    <Grid>
        <Border x:Name="KeyBorder" BorderBrush="White" BorderThickness="1" CornerRadius="5"
                Background="#00000000"
                MouseDown="keyBorder_MouseDown"
                MouseEnter="keyBorder_MouseEnter">
        </Border>
    </Grid>
</UserControl>
